{
    "name": "calvient/arbol",
    "description": "A simple data visualization tool for Laravel apps.",
    "keywords": [
        "Calvient",
        "laravel",
        "arbol"
    ],
    "homepage": "https://github.com/calvient/arbol",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Minson",
            "email": "jonathan@calvient.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0||^11.0",
        "inertiajs/inertia-laravel": "^1.2",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.1",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "Calvient\\Arbol\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Calvient\\Arbol\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Calvient\\Arbol\\ArbolServiceProvider"
            ],
            "aliases": {
                "Arbol": "Calvient\\Arbol\\Facades\\Arbol"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
